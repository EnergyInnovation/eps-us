Shifted Marginal Dispatch Cost by Hour by Optimization Pass[Electricity Timeslice,Hour\
		,current pass]=
	IF THEN ELSE(
	  Shifted Marginal Dispatch Cost by Hour by Optimization Pass[Electricity Timeslice,\
		Hour,preceeding pass] = 0,
	
	  0,
	
	  MAX(
	    IF THEN ELSE(
	      SUM(Electricity Dispatched by Least Cost by Hour by Optimization Pass[Electricity Timeslice\
		,Hour,Electricity Source!,preceeding pass])
	      < Unmet Electricity Demand After Zero or Negative Cost Dispatch[Electricity Timeslice\
		,Hour],
	  
	      Shifted Marginal Dispatch Cost by Hour by Optimization Pass[Electricity Timeslice\
		,Hour,preceeding pass]
	      + Price Optimization Step Size[Electricity Timeslice,Hour,current pass],
	
	      Shifted Marginal Dispatch Cost by Hour by Optimization Pass[Electricity Timeslice\
		,Hour,preceeding pass]
	      - Price Optimization Step Size[Electricity Timeslice,Hour,current pass]
	    ),
	
	    0
	  )
	)
	~	$/(MW*hr)
