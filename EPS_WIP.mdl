	~	Dmnl
	~		|

BAU Steps in Negative Direction for Least Cost Dispatch[Electricity Timeslice,Hour,pass1\
		]=
	0 ~~|
BAU Steps in Negative Direction for Least Cost Dispatch[Electricity Timeslice,Hour,current pass\
		]=
	IF THEN ELSE(
	  SUM(BAU Electricity Dispatched by Least Cost by Hour by Optimization Pass[Electricity Timeslice\
		,Hour,Electricity Source!,current pass])
	  > BAU Unmet Electricity Demand After Zero or Negative Cost Dispatch[Electricity Timeslice\
		,Hour],
	
	  BAU Steps in Negative Direction for Least Cost Dispatch[Electricity Timeslice,Hour\
		,preceeding pass] + 1,
	
	  0
	)
	~	Dmnl
	~		|

BAU Steps in Positive Direction for Least Cost Dispatch[Electricity Timeslice,Hour,pass1\
		]=
	0 ~~|
BAU Steps in Positive Direction for Least Cost Dispatch[Electricity Timeslice,Hour,current pass\
		]=
	IF THEN ELSE(
	  SUM(BAU Electricity Dispatched by Least Cost by Hour by Optimization Pass[Electricity Timeslice\
		,Hour,Electricity Source!,current pass])
	  < BAU Unmet Electricity Demand After Zero or Negative Cost Dispatch[Electricity Timeslice\
		,Hour],
	
	  BAU Steps in Positive Direction for Least Cost Dispatch[Electricity Timeslice,Hour\
		,preceeding pass] + 1,
	
	  0
	)
	~	Dmnl
	~		|

BAU Steps in Same Direction for Least Cost Dispatch[Electricity Timeslice,Hour,Optimization Pass\
		]=
	MAX(
	  BAU Steps in Positive Direction for Least Cost Dispatch[Electricity Timeslice,Hour\
		,Optimization Pass],
	  BAU Steps in Negative Direction for Least Cost Dispatch[Electricity Timeslice,Hour\
		,Optimization Pass]
	)
	~	Dmnl
	~		|

BAU Subsidy for Vehicle Battery Production=
