Minimum or Average Four Year Prior Total Energy Market Revenue by Plant Type Excluding Subsidies\
		[Electricity Source]=
	IF THEN ELSE(Time-INITIAL TIME <=4,MAX(
	  0,
	
	  IF THEN ELSE(
	    Time = INITIAL TIME + 1 :OR: Time = INITIAL TIME,
	
	    Total Revenue per Unit Output by Plant Type Excluding Subsidies[Electricity Source\
		],
	
	    IF THEN ELSE(
	      Time = INITIAL TIME + 2,
	
	      MIN(
	       Total Revenue per Unit Output by Plant Type Excluding Subsidies[Electricity Source\
		],
	        Previous Year Revenue per Unit New Elec Output[Electricity Source]
	      ),
	   
	      IF THEN ELSE(
	        Time = INITIAL TIME +3,
	
	      MIN(
	        MIN(
	          Total Revenue per Unit Output by Plant Type Excluding Subsidies[Electricity Source\
		],
	         Previous Year Revenue per Unit New Elec Output[Electricity Source]
	        ),
	        Two Year Prior Revenue per Unit New Elec Output[Electricity Source]
	      ),
	
	      MIN(
	        MIN(
	          MIN(
	          Total Revenue per Unit Output by Plant Type Excluding Subsidies[Electricity Source\
		],
	          Previous Year Revenue per Unit New Elec Output[Electricity Source]
	          ),
	         Two Year Prior Revenue per Unit New Elec Output[Electricity Source]
	        ),
	        Three Year Prior Revenue per Unit New Elec Output[Electricity Source]
	      )
	    )
	  )
	)),
	Four Year Average Energy Market Revenue by Electricity Source[Electricity Source])
	~	$/(MW*hr)
	~		|

